<Page navigatingTo="onNavigatingTo" xmlns="http://schemas.nativescript.org/tns.xsd">
    <ActionBar>
        <Label text="Senzor" />
    </ActionBar>

    <GridLayout rows="30, 30, 30, 30" columns="20,*,20">
        <Label text="{{ x }}" row="0" col="1" backgroundColor="#92a8d1"></Label>
        <Label text="{{ y }}" row="1" col="1" backgroundColor="#92a8d1"></Label>
        <Label text="{{ z }}" row="2" col="1" backgroundColor="#92a8d1"></Label>
        <Label text="{{ q }}" row="3" col="1" backgroundColor="#33baff"></Label>
    </GridLayout>
</Page>
